{
  "bindings": [
    {
      "type": "timerTrigger",
      "schedule": "*/15 * * * * *",
      "name": "timer",
      "direction": "in"
    },
    {
      "type": "httpTrigger",
      "authLevel": "anonymous",
      "direction": "in",
      "name": "req",
      "methods": ["get", "post"]
    },
    {
      "type": "http",
      "direction": "out",
      "name": "res"
    }
  ]
}
